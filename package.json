{
  "name": "spherical-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm run mon\" \"npm run app:serve\" \"npm run docs:serve\"",
    "exp:serve": "netlify-lambda serve src/app-server",
    "mon": "npx nodemon",
    "ts-check": "tsc --noEmit --lib ES2015 ./src/app-server/geo.ts",
    "build": "npm run app:build & npm run docs:build",
    "test:unit": "vue-cli-service test:unit --watch",
    "test:cyunit": "cypress open-ct",
    "lint": "vue-cli-service lint",
    "app:build": "vue-cli-service build",
    "app:serve": "concurrently \"vue-cli-service serve\" \"npx nodemon\"",
    "check:links": "vuepress check-md ./docs",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress dev docs --debug",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(ts|vue)' --locales './src/assets/languages/**/*.json'"
  },
  "dependencies": {
    "@firebase/auth-types": "^0.10.3",
    "@types/color": "^3.0.1",
    "@types/file-saver": "^2.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/three": "^0.135.0",
    "axios": "^0.24.0",
    "browserslist": "^4.19.1",
    "core-js": "^3.20.1",
    "detect-browser": "^5.2.0",
    "file-saver": "^2.0.5",
    "firebase": "^8.10.0",
    "lodash.clonedeep": "^4.5.0",
    "markdown-it-texmath": "^0.9.7",
    "newton-raphson-method": "^1.0.2",
    "node-gzip": "^1.1.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.0",
    "splitpanes": "^2.3.8",
    "three": "^0.136.0",
    "two.js": "0.7.13",
    "vue": "~2.6.14",
    "vue-advanced-cropper": "^1.7.0",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.24.4",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-web-cam": "^1.9.0",
    "vuetify": "^2.6.2",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@maginapp/vuepress-plugin-katex": "~1.0.7",
    "@mdi/font": "^6.5.95",
    "@mdi/js": "^6.5.95",
    "@types/jest": "~24.0.25",
    "@types/node": "^17.0.6",
    "@types/webpack": "~4.41.32",
    "@typescript-eslint/eslint-plugin": "~3.10.1",
    "@typescript-eslint/parser": "~3.10.1",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-dom": "^3.1.1",
    "@vue/eslint-config-typescript": "^6.0.0",
    "@vue/test-utils": "1.2.1",
    "@vuepress/plugin-back-to-top": "^1.5.2",
    "babel-plugin-transform-decorators": "^6.24.1",
    "concurrently": "^6.5.1",
    "eslint": "~6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "firestore-jest-mock": "https://github.com/dulimarta/firestore-jest-mock",
    "markdown-it-vuepress-code-snippet-enhanced": "^1.0.1",
    "netlify-lambda": "^2.0.15",
    "nodemon": "^2.0.15",
    "sass": "~1.32.13",
    "sass-loader": "~8.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.0.8",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "~2.6.14",
    "vuepress": "^1.5.3",
    "vuepress-plugin-check-md": "^0.0.2",
    "vuepress-plugin-mathjax": "^1.2.8",
    "vuepress-plugin-typescript": "^0.3.1",
    "vuepress-types": "^0.9.4",
    "vuetify-loader": "^1.6.0",
    "vuex-module-decorators": "^1.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "space-before-function-paren": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "packageManager": "yarn@3.1.1"
}
