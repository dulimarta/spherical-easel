{
  "debug.node.autoAttach": "on",
  "peacock.color": "#905dc6",
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.useEditorConfig": false,
  "prettier.requireConfig": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_attributes": "force",
      "wrap_line_length": "75",
      "indent_size": 2
    }
  },
  "vetur.format.options.tabSize": 2,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#aa83d3",
    "activityBar.activeBorder": "#e6cfb8",
    "activityBar.background": "#aa83d3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e6cfb8",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#905dc6",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#aa83d3",
    "titleBar.activeBackground": "#905dc6",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#905dc699",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#aa83d3",
    "statusBarItem.remoteBackground": "#905dc6",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "files.exclude": {
    "\"**/*.js\": {\"when\": \"$(basename).ts\"}": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*_latexmk": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.fls": true,
    "**/*.gz": true,
    "**/*.js.map": true,
    "**/*.log": true,
    "**/*.wasm": true
  },
  "cSpell.words": [
    "Labelable",
    "LINEANDSEGMENT",
    "linewidth",
    "midground",
    "Monospace",
    "noglow",
    "ONLYPOINTS",
    "Parametrics",
    "perp",
    "plottable",
    "plottables",
    "preimage",
    "renderable",
    "Resizeable",
    "SEGMENTSORLINEANDSEGMENT",
    "selectables",
    "Strikethrough",
    "Styleable",
    "unregistering",
    "untransform",
    "untransforming",
    "vuetify"
  ],
  "i18n-ally.localesPaths": [
    "docs/languages",
    "src/languages",
    "src/assets/languages"
  ],
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "cmake.configureOnOpen": false,
  "C_Cpp_Runner.cCompilerPath": "gcc",
  "C_Cpp_Runner.cppCompilerPath": "g++",
  "C_Cpp_Runner.debuggerPath": "gdb",
  "C_Cpp_Runner.cStandard": "",
  "C_Cpp_Runner.cppStandard": "",
  "C_Cpp_Runner.msvcBatchPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Auxiliary/Build/vcvarsall.bat",
  "C_Cpp_Runner.useMsvc": false,
  "C_Cpp_Runner.warnings": [
    "-Wall",
    "-Wextra",
    "-Wpedantic",
    "-Wshadow",
    "-Wformat=2",
    "-Wconversion",
    "-Wnull-dereference",
    "-Wsign-conversion"
  ],
  "C_Cpp_Runner.enableWarnings": true,
  "C_Cpp_Runner.warningsAsError": false,
  "C_Cpp_Runner.compilerArgs": [],
  "C_Cpp_Runner.linkerArgs": [],
  "C_Cpp_Runner.includePaths": [],
  "C_Cpp_Runner.includeSearch": [
    "*",
    "**/*"
  ],
  "C_Cpp_Runner.excludeSearch": [
    "**/build",
    "**/build/**",
    "**/.*",
    "**/.*/**",
    "**/.vscode",
    "**/.vscode/**"
  ],
  "C_Cpp_Runner.useAddressSanitizer": false
}