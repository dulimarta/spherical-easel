{
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.useEditorConfig": false,
  "prettier.requireConfig": false,
  "i18n-ally.pathMatcher": "{locale}.{ext}",
  "i18n-ally.localesPaths": [
    "./src/assets/languages"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "axios",
    "composables",
    "endregion",
    "firestore",
    "Labelable",
    "LINEANDSEGMENT",
    "linewidth",
    "Mathematica",
    "midground",
    "Monospace",
    "noglow",
    "ONLYPOINTS",
    "Parametrics",
    "perp",
    "pinia",
    "plottable",
    "plottables",
    "preimage",
    "renderable",
    "Resizeable",
    "Sected",
    "SEGMENTSORLINEANDSEGMENT",
    "selectables",
    "linethrough",
    "strikethrough",
    "Styleable",
    "unlisten",
    "unregistering",
    "untransform",
    "untransforming",
    "Vinicius",
    "vuetify"
  ],
  "eslint.validate": [
    "vue",
    "typescript",
    "javascript"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a1e26e",
    "activityBar.background": "#a1e26e",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "titleBar.activeBackground": "#86da43",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#86da4399",
    "titleBar.inactiveForeground": "#15202b99",
    "sash.hoverBorder": "#a1e26e",
    "activityBarBadge.background": "#4586da",
    "activityBarBadge.foreground": "#e7e7e7",
    "editorGroup.border": "#a1e26e",
    "panel.border": "#a1e26e",
    "sideBar.border": "#a1e26e",
    "statusBar.background": "#86da43",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#6cc426",
    "statusBarItem.remoteBackground": "#86da43",
    "statusBarItem.remoteForeground": "#15202b",
    "tab.activeBorder": "#a1e26e"
  },
  "peacock.color": "#86da43"
}