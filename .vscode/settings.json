{
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.useEditorConfig": false,
  "prettier.requireConfig": false,
  "i18n-ally.localesPaths": [
    "src/assets/languages"
  ],
  "i18n-ally.disablePathParsing": false,
  "i18n-ally.pathMatcher": "{locale}.{ext}",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "axios",
    "composables",
    "endregion",
    "firestore",
    "Labelable",
    "LINEANDSEGMENT",
    "linewidth",
    "Mathematica",
    "midground",
    "Monospace",
    "noglow",
    "ONLYPOINTS",
    "Parametrics",
    "perp",
    "pinia",
    "plottable",
    "plottables",
    "preimage",
    "renderable",
    "Resizeable",
    "Sected",
    "SEGMENTSORLINEANDSEGMENT",
    "selectables",
    "linethrough",
    "strikethrough",
    "Styleable",
    "unlisten",
    "unregistering",
    "untransform",
    "untransforming",
    "Vinicius",
    "vuetify"
  ],
  "eslint.validate": ["vue", "typescript", "javascript"],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#a1e26e",
    "activityBar.background": "#a1e26e",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#a1e26e",
    "titleBar.activeBackground": "#86da43",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#86da4399",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#86da43",
  "workbench.editor.enablePreview": false,
  "i18n-ally.keystyle": "nested",
  "editor.formatOnSave": true
}