{
  "debug.node.autoAttach": "on",
  "peacock.color": "#905dc6",
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.useEditorConfig": false,
  "prettier.requireConfig": false,
  // "vetur.experimental.templateInterpolationService": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "vetur.format.defaultFormatter.ts": "prettier-tslint",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_attributes": "force",
      "wrap_line_length": "75",
      "indent_size": 2
    }
  },
  "vetur.format.options.tabSize": 2,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#aa83d3",
    "activityBar.activeBorder": "#e6cfb8",
    "activityBar.background": "#aa83d3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e6cfb8",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#905dc6",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#aa83d3",
    "titleBar.activeBackground": "#905dc6",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#905dc699",
    "titleBar.inactiveForeground": "#e7e7e799",
    "sash.hoverBorder": "#aa83d3",
    "statusBarItem.remoteBackground": "#905dc6",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "\"**/*.js\": {\"when\": \"$(basename).ts\"}": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*_latexmk": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.fls": true,
    "**/*.gz": true,
    "**/*.js.map": true,
    "**/*.log": true,
    "**/*.wasm": true
  },
  "cSpell.words": [
    "endregion",
    "Labelable",
    "LINEANDSEGMENT",
    "linewidth",
    "midground",
    "Monospace",
    "noglow",
    "ONLYPOINTS",
    "Parametrics",
    "perp",
    "pinia",
    "plottable",
    "plottables",
    "preimage",
    "renderable",
    "Resizeable",
    "SEGMENTSORLINEANDSEGMENT",
    "selectables",
    "Strikethrough",
    "Styleable",
    "unlisten",
    "unregistering",
    "untransform",
    "untransforming",
    "vuetify"
  ],
  "i18n-ally.localesPaths": ["./src/assets/languages"],
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.readonlyFromPermissions": false,
  "files.readonlyInclude": {
    "node_modules/**": true
  },
  "i18n-ally.annotations": true,
  "i18n-ally.autoDetection": true,
  "i18n-ally.disablePathParsing": false,
  "i18n-ally.displayLanguage": "en,id",
  "i18n-ally.sourceLanguage": "en,id",
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.extract.keyMaxLength": 50,
  "i18n-ally.frameworks.ruby-rails.scopeRoot": "",
  "i18n-ally.fullReloadOnChanged": true,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.translate.promptSource": true,
  "i18n-ally.dirStructure": "dir",
  "i18n-ally.enabledParsers": ["ts"],
  "i18n-ally.enabledFrameworks": ["vue-sfc", "vue"],
  "i18n-ally.extract.keygenStyle": "camelCase",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.editor.preferEditor": true
}
